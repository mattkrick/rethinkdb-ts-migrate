{
  "name": "rethinkdb-ts-migrate",
  "version": "0.3.0",
  "description": "A migration tool for RethinkDB-ts",
  "main": "lib/migrate",
  "scripts": {
    "lint": "eslint --fix src/**/*.ts",
    "precommit": "lint-staged && tsc",
    "prepublish": "tsc"
  },
  "keywords": [
    "rethinkdb",
    "migrations"
  ],
  "author": "Matt Krick <matt.krick@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattkrick/rethinkdb-ts-migrate"
  },
  "bugs": {
    "url": "https://github.com/mattkrick/rethinkdb-ts-migrate/issues",
    "email": "matt.krick@gmail.com"
  },
  "bin": "./bin/migrate-rethinkdb",
  "dependencies": {
    "chalk": "^1.1.3",
    "meow": "^10.1.2",
    "moment": "^2.13.0",
    "nconf": "^0.11.3",
    "rethinkdb-ts": "^2.4.5",
    "sucrase": "^3.10.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/nconf": "^0.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "chai": "^3.5.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "prettier": "^2.5.0",
    "rethinkdb": "^2.3.0",
    "typescript": "^4.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "printWidth": 100,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
